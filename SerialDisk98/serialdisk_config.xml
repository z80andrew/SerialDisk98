<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <LocalDirectoryPath>Atari</LocalDirectoryPath>
  <LoggingLevel>Info</LoggingLevel>
  <IsCompressionEnabled>true</IsCompressionEnabled>

  <SerialSettings>
    <PortName>COM1</PortName>
    <BaudRate>9600</BaudRate>
    <DataBits>8</DataBits>
    <StopBits>One</StopBits>
    <Parity>None</Parity>
    <Handshake>None</Handshake>
  </SerialSettings>

  <DiskSettings>
    <DiskSizeMiB>32</DiskSizeMiB>
    <DiskPartitionType>GEM</DiskPartitionType>
    <DiskTOSCompatibility>TOS104</DiskTOSCompatibility>
    <RootDirectorySectors>8</RootDirectorySectors>
  </DiskSettings>
</configuration>